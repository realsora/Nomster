<%= @places.each do |place|%>
<div class="booyah-box col-10 offset-1">
  <h1><%= place.name %></h1>
  <i><%= place.address %></i> <br/><br/>
  <p><%= place.description %></p> <br/>
</div>
  <% end %>
  <hr/>
  <div class="paging col-3 offset-5">
  <%= will_paginate @places, renderer: BootstrapPagination::Rails %>
  </div>
    



 